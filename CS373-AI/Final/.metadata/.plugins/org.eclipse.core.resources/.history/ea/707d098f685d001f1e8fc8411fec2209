package project;


public enum KingAction {
        
		RIGHT(1, 0) ,
        DOWN(0, -1),
        LEFT(-1, 0),
        UP(0, 1);


        private int xChange, yChange;

        private KingAction(int xChange, int yChange) {
            this.xChange = xChange;
            this.yChange = yChange;
        }

        public int getActionId() {
            return ordinal();
        }

    }
	
	
}
